{
  "id": "spell-book",
  "title": "Spell Book",
  "description": "Enhanced spell management for DnD5e. Easily view, prepare, and customize spell lists.",
  "version": "1.0.0",
  "bugs": "https://github.com/Sayshal/spell-book/issues",
  "changelog": "https://github.com/Sayshal/spell-book/releases",
  "license": "https://github.com/Sayshal/spell-book?tab=MIT-1-ov-file",
  "readme": "https://github.com/Sayshal/spell-book?tab=readme-ov-file",
  "authors": [
    {
      "name": "Tyler",
      "discord": "Tyler",
      "url": "https://github.com/Sayshal",
      "flags": {}
    }
  ],
  "compatibility": {
    "maximum": "13.340",
    "minimum": "12.331",
    "verified": "12.331"
  },
  "download": "",
  "esmodules": ["scripts/spell-book.mjs", "scripts/constants.mjs", "scripts/hooks.mjs"],
  "flags": {
    "hotReload": {
      "extensions": ["css", "html", "hbs", "json"],
      "paths": ["styles", "lang", "templates"]
    }
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "manifest": "https://github.com/Sayshal/spell-book/releases/latest/download/module.json",
  "relationships": {
    "systems": [
      {
        "compatibility": {
          "maximum": "4.4.x",
          "minimum": "3.3.1",
          "verified": "3.3.1"
        },
        "id": "dnd5e",
        "type": "system"
      }
    ]
  },
  "packs": [
    {
      "name": "custom-spell-lists",
      "label": "Custom Spell Lists",
      "type": "JournalEntry",
      "path": "storage/custom-spell-lists",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    }
  ],
  "packFolders": [
    {
      "name": "Spell Book",
      "sorting": "a",
      "packs": ["custom-spell-lists"]
    }
  ],
  "styles": ["styles/spell-book.css"],
  "url": "https://github.com/Sayshal/spell-book",
  "hasStorage": true
}
